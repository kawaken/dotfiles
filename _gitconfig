[alias]
    ad = add
    br = branch
    ci = commit
    cl = config --list
    co = checkout
    df = diff
    dif = diff
    hist = log --pretty=oneline --graph --date=short
    in = fetch --dry-run
    last = log -1 HEAD
    lg = log --pretty=short --graph
    log-all = log --graph --all --decorate
    ls = ls-files
