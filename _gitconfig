[user]
    name = 
    email = 
[include]
    path = .gitconfig.local
[core]
    pager = less -R
    excludesfile = ~/.gitignore-global
    autocrlf = input
    quotepath = false
    filemode = false
[color]
    ui = true
[push]
    default = simple
[alias]
    ad = add
    br = branch
    ci = commit
    cl = config --list
    co = checkout
    df = diff
    fe = fetch
    hist = log --pretty=format:'%C(yellow)%h %C(blue)%ad%Creset %s' --graph --date=short
    in = fetch --dry-run
    last = show HEAD
    lg = log --decorate --stat --all --graph --date=iso
    ls = ls-files
    mg = merge
    mo = merge origin/master
    out = push -n -v
    pul = pull --ff-only
    pus = push
    rv = remote -v
    st = status -s
    sh = show
    today = log --oneline --since='12 hour ago'
    us = reset HEAD # unstage
    ex = exclude
    sts = stash
    stsu = stash -u

[github]
	user = kawaken
[init]
	templatedir = ~/.git-hooks
